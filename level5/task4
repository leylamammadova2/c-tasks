#include <iostream>
using namespace std;
void max(int a, int b, int c) {
    if (a >= b && a >= c)
        cout << "en boyuk = " << a << endl;
    else if (b >= a && b >= c)
        cout << "en boyuk = " << b << endl;
    else
        cout << "en boyuk = " << c << endl;
}
int main() {
    int a, b, c;
    cout << "ededleri daxil et: ";
    cin >> a >> b >> c;
    max(a, b, c);
    return 0;
}
