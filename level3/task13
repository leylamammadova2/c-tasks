#include <iostream>
using namespace std;

int main() {
    int N, x;
    cin >> N;

    int even = 0, odd = 0;

    for (int i = 0; i < N; i++) {
        cin >> x;
        if (i % 2 == 0) even += x;
        else odd += x;
    }

    cout << "Cut indekslerin cemi: " << even << endl;
    cout << "Tek indekslerin cemi: " << odd << endl;
}