#include <iostream>
using namespace std;
int main() {
    int a;
    cout << "bali daxil et:";
    cin >> a;
    if (a >= 0 & a <= 50) cout << "F";
    else if (a <= 60) cout << "E";
    else if (a <= 70) cout << "D";
    else if (a <= 80) cout << "C";
    else if (a <= 90) cout << "B";
    else  cout << "A";

    return 0;
}