#include <iostream>
using namespace std;

int main() {
    int n, rev = 0;
    cin >> n;

    int org = n;

    while (n > 0) {
        rev = rev * 10 + n % 10;
        n /= 10;
    }

    cout << "Ters eded: " << rev << endl;

    if (rev == org) cout << "Eded palindromdur.";
    else cout << "Eded palindrom deyil.";
}